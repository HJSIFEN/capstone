    "import re\n",
    "\n",
    "def site_popularity(link):\n",
    "    if(re.search(\"naver\", link)):\n",
    "        return 10\n",
    "    elif(re.search(\"daum\", link)):\n",
    "        return 5\n",
    "    elif(re.search(\"tistory\", link)):\n",
    "        return 3\n",
    "    else:\n",
    "        return 1"
   ]
